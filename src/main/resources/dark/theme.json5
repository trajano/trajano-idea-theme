//    "selectionForeground": "foreground",
    "primaryRed": "#821010",
    "primaryBlue": "#2020cc",
    "primaryYellow": "#cccc20",
    "backgroundDefault": "#151519",
    "tabBackgroundDefault": "#951519",
    "toolBackgroundDefault": "#000000",
    // slight red-green tinge to reduce blue output
    "disabled": "#777777",
    "disabledText": "#777777",
    // panels are black, they're not the focus of the work.
    "panel": "#000000",
