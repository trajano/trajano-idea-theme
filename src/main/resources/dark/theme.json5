  "ui": {
    "*": {
      "background": "panel",
      "disabledForeground": "disabled",
      "foreground": "foreground",
      "inactiveUnderlineColor": "indigo-600",
      "paintBorder": false,
      "underlineColor": "blue-600"
    },
    "Button": {
      "default": {
        // "shadowColor"
      }
    },
    "Checkbox": {
      "background": "panel"
    },
    "Component": {
      //      "arc": 0
    },
    "DefaultTabs": {
      "underlinedTabBackground": "activeBackground"
    },
    "EditorTabs": {
      "background": "panel",
      "hoverBackground": "activeBackground",
      "hoverInactiveBackground": "activeBackground",
      "hoverSelectedBackground": "activeBackground",
      "hoverSelectedInactiveBackground": "activeBackground",
      "inactiveColoredFileBackground": "backgroundDefault",
      "underlineArc": 4,
      "underlineHeight": 4
    },
    "List": {
      "border": "4,0,4,0",
      "rowHeight": 24
      //          "selectionBackground": "primaryRed"
    },
    "MainMenu": {
      "background": "menu",
      "foreground": "foreground",
      "selectionBackground": "menuSelectionBackground"
    },
    "Menu": {
      "background": "menu",
      "borderColor": "lightBorder",
      "disabledBackground": "menu",
      "maxGutterIconWidth": 18,
      "opaque": true,
      "selectionBackground": "menuSelectionBackground",
      "separatorColor": "separator"
      //      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaMenuItemBorder"
    },
    "MenuBar": {
      "Icon": {
        "insets": "5,5,5,5"
      },
      "background": "menu",
      "disabledBackground": "menu",
      "highlight": "activeBackground",
      "selectionBackground": "menuSelectionBackground",
      "separatorColor": "separator"
      //      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaMenuBarBorder"
    },
    "MenuItem": {
      "background": "menu",
      "disabledBackground": "menu",
      "selectionBackground": "menuSelectionBackground"
    },
    "Notification": {
      "ToolWindow": {
        "errorBackground": "primaryRed",
        "errorBorderColor": "primaryRed",
        "informativeBackground": "menu",
        "informativeBorderColor": "menu",
        "warningBackground": "menu",
        "warningBorderColor": "menu"
      },
      "background": "menu"
    },
    "Popup": {
      "Header": {
        "activeBackground": "activeBackground",
        "background": "menu",
        "inactiveBackground": "primaryBlue"
      },
      "background": "menu",
      "paintBorder": false
    },
    "PopupMenu": {
      "background": "menu",
      "disabledBackground": "menu",
      "selectionBackground": "menuSelectionBackground"
    },
    "SearchEverywhere": {
      "Header": {
        "activeBackground": "primaryRed",
        "background": "menu",
        "inactiveBackground": "menu"
      },
      "Tab": {
        "background": "menu",
        "inactiveBackground": "primaryBlue",
        "selectedBackground": "activeBackground"
      },
      "ToolWindow": {
        "background": "menu"
      },
      "background": "menu"
    },
    "Separator": {
      "separatorColor": "separator"
    },
    "TabbedPane": {
      "activeBackground": "primaryRed",
      "focusColor": "primaryRed",
      "inactiveBackground": "primaryBlue",
      "tabHeight": 40,
      "tabSelectionArc": 4
    },
    "ToolWindow": {
      "Button": {
        "background": "primaryBlue",
        "inactiveBackground": "primaryRed"
      },
      "DragAndDrop": {
        "areaBackground": "primaryRed"
      },
      "Header": {
        "background": "primaryBlue",
        "inactiveBackground": "panel"
      },
      "HeaderTab": {
        "background": "primaryBlue",
        "inactiveBackground": "primaryRed"
      },
      "terminal": {
        "background": "terminalBackground"
      }
    }
