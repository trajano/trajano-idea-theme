  "ui": {
    "*": {
      "background": "panel",
      "foreground": "foreground",
      "disabledForeground": "disabled",
      "underlineColor": "blue-600",
      "inactiveUnderlineColor": "indigo-600",
      "paintBorder": false
    },
    "Component": {
      //      "arc": 0
    },
    "DefaultTabs": {
      "underlinedTabBackground": "activeBackground"
    },
    "EditorTabs": {
      // active and hovering tab, no need for separate colors.
      // the underline will also show active tab
      "background": "panel",
      "underlinedTabBackground": "activeBackground",
      "hoverSelectedBackground": "activeBackground",
      "hoverBackground": "activeBackground",
      "hoverInactiveBackground": "activeBackground",
      "hoverSelectedInactiveBackground": "activeBackground",
      //      "EditorTabs.inactiveBackground": "backgroundDefault",
      "inactiveColoredFileBackground": "backgroundDefault",

      "underlineArc": 4,
      "underlineHeight": 4
    },
    "List": {
      "rowHeight": 24,
      "border": "4,0,4,0",
      //          "selectionBackground": "primaryRed"
    },
