      //      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaMenuItemBorder",
      "selectionBackground": "menuSelectionBackground",
      "borderColor": "lightBorder",
      "maxGutterIconWidth": 18,
      "opaque": true,
      "separatorColor": "separator",
      "background": "menu",
      "disabledBackground": "menu",
    },
    "MenuItem": {
      "background": "menu",
      "disabledBackground": "menu",
      "selectionBackground": "menuSelectionBackground",
    },
    "Popup": {
      "background": "menu",
      "paintBorder": false,
      "Header": {
        "background": "menu",
        "activeBackground": "primaryRed",
        "inactiveBackground": "primaryBlue",
      }
    },
    "PopupMenu": {
      "background": "menu",
      "disabledBackground": "menu",
      "selectionBackground": "menuSelectionBackground",
    },
    "MainMenu": {
      "background": "menu",
      "foreground": "foreground",
      "selectionBackground": "menuSelectionBackground"
    },
    "MenuBar": {
      "Icon": {
        "insets": "5,5,5,5"
      },
      //      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaMenuBarBorder",
      "background": "menu",
      "disabledBackground": "menu",
      "selectionBackground": "menuSelectionBackground",
      "separatorColor": "separator",
      "highlight": "activeBackground",
    },
    "Separator": {
      "separatorColor": "separator"
    },
    "SearchEverywhere": {
      "background": "menu",
      "Header": {
        "background": "menu",
        "activeBackground": "primaryRed",
        "inactiveBackground": "primaryBlue"
      },
      "Tab": {
        "background": "menu",
        "selectedBackground": "activeBackground",
        "inactiveBackground": "primaryBlue"
      },
      "ToolWindow": {
        "background": "menu"
      }
    },
    "TabbedPane": {
      "tabHeight": 40,
      "tabSelectionArc": 4,
      "activeBackground": "primaryRed",
      "inactiveBackground": "primaryBlue",
      "focusColor": "primaryRed"
    },
    "ToolWindow": {
      "Header": {
        "background": "primaryBlue",
        "inactiveBackground": "panel"
      },
      "HeaderTab": {
        "background": "primaryBlue",
        "inactiveBackground": "primaryRed"
      },
      "Button": {
        "background": "primaryBlue",
        "inactiveBackground": "primaryRed"
      },
      "DragAndDrop": {
        "areaBackground": "primaryRed"
      }
    },
    "Notification": {
      "background": "menu",
      "ToolWindow": {
//        "errorForeground": "Gray12",
//        "warningForeground": "Gray12",
//        "informativeForeground": "Gray12",
